<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="org.noodle.mapper.MemberMapper">
  <insert id="create">
  insert into tbl_user(mno,userid,userpw,username,nickname,email,picture)
  values
  (seq_user.nextval, #{userid},#{userpw},#{username},#{nickname},#{email},#{picture})
  
  </insert>
  
  
  
  </mapper>